{
    "name": "bobalazek/symfony5-boilerplate",
    "description": "Symfony5 boilerplate",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "cocur/slugify": "^4.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "easycorp/easyadmin-bundle": "^3.0",
        "endroid/qr-code": "^3.7",
        "facebook/graph-sdk": "^5.7",
        "google/apiclient": "^2.4",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "karser/karser-recaptcha3-bundle": "^0.1.5",
        "knplabs/doctrine-behaviors": "^2.0",
        "knplabs/knp-paginator-bundle": "^5.4",
        "league/flysystem-bundle": "^1.5",
        "php-translation/symfony-bundle": "^0.12.0",
        "picqer/php-barcode-generator": "^2.0",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^4.0",
        "scienta/doctrine-json-functions": "^4.1",
        "sensio/framework-extra-bundle": "^5.5",
        "sonata-project/google-authenticator": "^2.2",
        "symfony/asset": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.6.2",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/http-client": "5.2.*",
        "symfony/inflector": "5.2.*",
        "symfony/intl": "5.2.*",
        "symfony/mailer": "5.2.*",
        "symfony/messenger": "5.2.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/security-csrf": "5.2.*",
        "symfony/serializer-pack": "*",
        "symfony/translation": "5.2.*",
        "symfony/twig-bundle": "5.2.*",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "5.2.*",
        "symfony/web-link": "5.2.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.2.*",
        "twig/cssinliner-extra": "^3.0",
        "twig/extensions": "^1.5",
        "twig/extra-bundle": "^3.0",
        "twig/inky-extra": "^3.0",
        "twig/intl-extra": "^3.0",
        "vich/uploader-bundle": "^1.16"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.3",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.29",
        "phpstan/phpstan-doctrine": "^0.12.16",
        "phpstan/phpstan-phpunit": "^0.12.11",
        "phpstan/phpstan-symfony": "^0.12.6",
        "symfony/browser-kit": "5.2.*",
        "symfony/css-selector": "5.2.*",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "database-recreate": [
            "php bin/console doctrine:schema:drop --force",
            "php bin/console doctrine:schema:update --force",
            "php bin/console doctrine:fixtures:load --no-interaction",
            "php bin/console doctrine:schema:validate"
        ],
        "lint": [
            "php bin/console lint:twig templates/",
            "php bin/console lint:yaml config/",
            "php bin/console lint:xliff translations/",
            "php-cs-fixer fix --dry-run --diff",
            "vendor/bin/phpstan analyse -c phpstan.neon"
        ],
        "test": [
            "php bin/phpunit"
        ],
        "php-cs-fix": [
              "php-cs-fixer fix"
        ],
        "translations-generate": [
              "php bin/console translation:update --dump-messages en --force"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.2.*"
        }
    }
}
