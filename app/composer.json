{
    "name": "bobalazek/corcoviewer",
    "description": "Corcoviewer project",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "cocur/slugify": "^4.0",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "easycorp/easyadmin-bundle": "^2.3",
        "endroid/qr-code": "^3.7",
        "facebook/graph-sdk": "^5.7",
        "google/apiclient": "^2.4",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "karser/karser-recaptcha3-bundle": "^0.1.5",
        "knplabs/doctrine-behaviors": "^2.0",
        "knplabs/knp-paginator-bundle": "^5.1",
        "noxlogic/ratelimit-bundle": "1.x",
        "php-translation/symfony-bundle": "^0.12.0",
        "picqer/php-barcode-generator": "^2.0",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^4.0",
        "scienta/doctrine-json-functions": "^4.1",
        "sensio/framework-extra-bundle": "^5.5",
        "snc/redis-bundle": "^3.2",
        "sonata-project/google-authenticator": "^2.2",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.6.2",
        "symfony/form": "5.1.*",
        "symfony/framework-bundle": "5.1.*",
        "symfony/http-client": "5.1.*",
        "symfony/inflector": "5.1.*",
        "symfony/intl": "5.1.*",
        "symfony/mailer": "5.1.*",
        "symfony/messenger": "5.1.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/security-csrf": "5.1.*",
        "symfony/serializer-pack": "*",
        "symfony/translation": "5.1.*",
        "symfony/twig-bundle": "5.1.*",
        "symfony/validator": "5.1.*",
        "symfony/web-link": "5.1.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.1.*",
        "twig/cssinliner-extra": "^3.0",
        "twig/extensions": "^1.5",
        "twig/extra-bundle": "^3.0",
        "twig/inky-extra": "^3.0",
        "twig/intl-extra": "^3.0",
        "vich/uploader-bundle": "^1.14"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.3",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.29",
        "phpstan/phpstan-doctrine": "^0.12.16",
        "phpstan/phpstan-phpunit": "^0.12.11",
        "phpstan/phpstan-symfony": "^0.12.6",
        "symfony/browser-kit": "5.1.*",
        "symfony/css-selector": "5.1.*",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.1.*"
        }
    }
}
